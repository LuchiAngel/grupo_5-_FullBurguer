<!DOCTYPE html>
<html lang="en">
    <%- include('../partials/head.ejs')%><link rel="stylesheet" href="/css/productList.css">
<body>
    <%- include('../partials/header.ejs')%>
 <main>
    <section>
        <h3 class="titular-detail">Hamburguesas 100% hechas con carne de verdad</h3>
        <div class="combo-container">
            <% combo.forEach((combo)=>{%>
              <article class="combo">
                <h4 class="detail"><%= combo.name %></h4>                          
                <a href="productDetail/<%= combo.id %>"><img class="fotos" src=../images/<%= combo.images %> /></a>
                            
            </article>  <%})%>
          </div>
              
    </section>

 </main>
  
</body>
  <%- include('../partials/footer.ejs')%>
</html>